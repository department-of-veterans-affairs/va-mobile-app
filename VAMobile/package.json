{
  "name": "va-health-and-benefits",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start:metro-server": "react-native start --reset-cache",
    "start": "yarn run env:staging && concurrently \"yarn run start:metro-server\" \"yarn run lint:watch\"",
    "test:watch": "jest --coverage --colors --watchAll",
    "test:debug": "yarn run env:test && jest --colors",
    "test": "yarn run env:test && jest --coverage --colors --silent",
    "test:integration": "yarn run env:test && yarn yarn run build:test && wdio ./wdioConf/wdio.conf.js",
    "test:integration-android": "yarn run env:test && yarn run build:android-test && TEST_PLATFORM=android wdio ./wdioConf/wdio.conf.js",
    "test:integration-ios": "yarn run env:test && yarn run build:ios-test && TEST_PLATFORM=ios npx wdio ./wdioConf/wdio.conf.js",
    "build:test": "concurrently \"yarn run build:android-test\" \"yarn run build:ios-test\"",
    "build:android-test": "yarn run bundle:android && cd android; ./gradlew assembleIntegrationtestDebug",
    "build:ios-test": "yarn run bundle:ios && cd ios; xcodebuild -configuration Release -workspace VAMobile.xcworkspace -scheme VAMobileRelease -destination 'platform=iOS Simulator,name=iPhone 11,OS=14.4' -derivedDataPath ./output -sdk iphonesimulator BUILD_LIBRARIES_FOR_DISTRIBUTION=YES build",
    "clean": "yarn run clean:android && yarn run clean:ios",
    "clean:android": "cd android; ./gradlew clean",
    "clean:ios": "rm -rf ./ios/output",
    "bundle:android": "react-native bundle --reset-cache --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle:ios": "react-native bundle --reset-cache --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios",
    "lint:watch": "watch --wait=2 'yarn run lint:forWatcher' ./src",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "tsc:compile": "tsc -p ./tsconfig.app.json",
    "lint:fix": "eslint --fix ./src --ext .js,.jsx,.ts,.tsx",
    "lint:ci": "eslint --max-warnings 0 ./src --ext .js,.jsx,.ts,.tsx && yarn run tsc:compile",
    "lint:forWatcher": "yarn run lint:fix && yarn run tsc:compile || exit 0",
    "jest:clear": "jest --clearCache",
    "env:staging": "./env/env.sh -e staging -d true",
    "env:test": "./env/env.sh -e staging -d true -t true",
    "env:uat": "./env/env.sh -d true -t false",
    "env:production": "./env/env.sh -e production -d false -t false",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.9.0",
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^3.2.0",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-cookies/cookies": "^6.0.11",
    "@react-native-firebase/analytics": "^12.0.0",
    "@react-native-firebase/app": "^12.0.0",
    "@react-native-firebase/crashlytics": "^12.0.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "eslint-plugin-tsdoc": "^0.2.11",
    "i18next": "^21.3.3",
    "jsc-android": "^250230.2.1",
    "jwt-decode": "^3.1.2",
    "luxon": "^2.0.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "querystringify": "^2.2.0",
    "react": "^17.0.2",
    "react-i18next": "^11.8.10",
    "react-native": "^0.66.1",
    "react-native-document-picker": "^7.1.1",
    "react-native-dotenv": "^3.2.0",
    "react-native-file-viewer": "^2.1.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-image-picker": "^4.1.2",
    "react-native-keyboard-manager": "^6.5.4-3",
    "react-native-keychain": "^7.0.0",
    "react-native-localize": "^2.0.2",
    "react-native-reanimated": "^2.2.3",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-webview": "^11.2.5",
    "react-navigation": "^4.4.4",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.12.0",
    "styled-components": "^5.2.1",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/jest-when": "^2.7.3",
    "@types/luxon": "^2.0.4",
    "@types/mocha": "^9.0.0",
    "@types/querystringify": "^2.0.0",
    "@types/react-native": "^0.65.9",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.8",
    "@types/redux-mock-store": "^1.0.2",
    "@types/styled-components-react-native": "^5.1.1",
    "@types/underscore": "^1.11.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@wdio/appium-service": "^7.13.0",
    "@wdio/cli": "^7.13.0",
    "@wdio/local-runner": "^7.13.0",
    "@wdio/mocha-framework": "^7.13.0",
    "@wdio/spec-reporter": "^7.13.0",
    "appium": "^1.22.0",
    "appium-doctor": "^1.16.0",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "chromedriver": "^93.0.1",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-native": "^3.9.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jest-styled-components": "^7.0.5",
    "jest-when": "^3.4.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-devtools": "^4.20.2",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.3.1",
    "typescript": "^4.4.3",
    "watch": "^1.0.2",
    "wdio-chromedriver-service": "^7.2.2",
    "webdriverio": "^7.13.0"
  },
  "jest-junit": {
    "suiteName": "vamobile",
    "outputName": "junit-vamobile.xml",
    "suiteNameTemplate": "vamobile.\"{title}\"",
    "classNameTemplate": "vamobile.\"{classname}\"",
    "outputDirectory": "./coverage/junit"
  }
}
