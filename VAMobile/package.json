{
  "name": "VAMobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start:metro-server": "react-native start --reset-cache",
    "start": "DOT_ENV_FILE=./env/int.env concurrently \"npm run start:metro-server\" \"npm run lint:watch\"",
    "test:watch": "jest --coverage --colors --watchAll",
    "test": "DOT_ENV_FILE=./env/test.env jest --coverage --colors --silent",
    "test:integration": "npm run test:integration-ios && test:integration-android",
    "test:integration-ios": "DOT_ENV_FILE=./env/int.env npx wdio ./wdioConf/wdio.ios.conf.js",
    "test:integration-android": "DOT_ENV_FILE=./env/int.env wdio ./wdioConf/wdio.android.conf.js",
    "build:android-debug": "cd android; ./gradlew clean; ./gradlew :app:assembleDebug",
    "start:appium": "npx appium",
    "lint:watch": "watch --wait=2 'npm run lint:forWatcher' ./src",
    "lint": "eslint --fix ./src --ext .js,.jsx,.ts,.tsx && tsc -p ./tsconfig.app.json",
    "lint:forWatcher": "eslint --fix ./src --ext .js,.jsx,.ts,.tsx && tsc -p ./tsconfig.app.json || exit 0",
    "jest:clear": "jest --clearCache"
  },
  "dependencies": {
    "@react-native-community/cookies": "^4.0.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.9.0",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "@types/mocha": "^8.0.3",
    "@types/react-redux": "^7.1.9",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.3",
    "@types/underscore": "^1.10.24",
    "appium-doctor": "^1.15.3",
    "jwt-decode": "^3.0.0-beta.2",
    "querystringify": "^2.2.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-dotenv": "^2.4.0",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keychain": "^6.2.0",
    "react-native-reanimated": "^1.13.0",
    "react-native-responsive-screen": "^1.4.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-webview": "^10.0.0",
    "react-navigation": "^4.4.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0",
    "underscore": "^1.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^26.0.14",
    "@types/querystringify": "^2.0.0",
    "@types/react-native": "^0.63.2",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux-mock-store": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@wdio/appium-service": "^6.4.7",
    "@wdio/cli": "^6.5.2",
    "@wdio/local-runner": "^6.5.2",
    "@wdio/mocha-framework": "^6.5.0",
    "@wdio/spec-reporter": "^6.4.7",
    "appium": "^1.18.2",
    "babel-jest": "^26.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "chromedriver": "^85.0.1",
    "concurrently": "^5.3.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-react-native": "^3.9.1",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^26.4.0",
    "jest-junit": "^11.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-devtools": "^4.8.2",
    "react-test-renderer": "16.13.1",
    "redux-mock-store": "^1.5.4",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.7",
    "watch": "^1.0.2",
    "wdio-chromedriver-service": "^6.0.4",
    "webdriverio": "^6.5.2"
  },
  "jest-junit": {
    "suiteName": "vamobile",
    "outputName": "junit-vamobile.xml",
    "suiteNameTemplate": "vamobile.\"{title}\"",
    "classNameTemplate": "vamobile.\"{classname}\"",
    "outputDirectory": "./coverage/junit"
  }
}
