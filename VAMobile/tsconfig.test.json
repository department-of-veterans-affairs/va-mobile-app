{
	"extends": "./tsconfig",
	"compilerOptions": {
		"baseUrl": "src",
		"outDir": "dist/",
		"allowJs": true,
		"checkJs": false,
		"moduleResolution": "node",
		"sourceMap": false,
		"noImplicitAny": false,
		"target": "es6",
		"module": "commonjs",
		"lib": [
			"es5",
			"es6",
			"dom"
		],
		"jsx": "react",
		"strict": false,
		"removeComments": true,
		"types": [
			"node",
			"webdriverio",
			"jest"
		],
		"paths": {
			
		}
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	],
	"include": [
		"global.d.ts",
		"src/testUtils.ts",
		"src"
	]
}